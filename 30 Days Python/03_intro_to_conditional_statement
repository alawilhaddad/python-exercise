# https://www.hackerrank.com/challenges/30-conditional-statements/problem
if __name__ == '__main__':
    N = int(input().strip())
    if N % 2 == 1:
        print('Weird')
    else:
        if 2 <= N <= 5:
            print('Not Weird')
        elif 6 <= N <= 20:
            print('Weird')
        elif 20 < N:
            print('Not Weird')
